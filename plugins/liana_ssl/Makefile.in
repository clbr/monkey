all: monkey-liana_ssl.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS -DPOSIX
OBJECTS = liana_ssl.o
MATRIX_HEADERS = /usr/include/matrixssl/

-include $(OBJECTS:.o=.d)

monkey-liana_ssl.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lmatrixssl
